{
  "name": "doctocat-nextjs-site",
  "version": "0.0.13",
  "private": true,
  "description": "Documentation theme",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "check": "tsc --noEmit",
    "build": "rm -rf out && next build --webpack",
    "lint": "eslint '**/*.{js,ts,tsx,md,mdx}' --max-warnings=0",
    "format": "prettier --write '**/*.{js,ts,tsx,md,mdx,json,css,scss}'",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primer/doctocat-nextjs.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/primer/doctocat-nextjs/issues"
  },
  "homepage": "https://github.com/primer/doctocat-nextjs#readme",
  "dependencies": {
    "@primer/doctocat-nextjs": "^0.9.0",
    "@primer/octicons-react": "19.15.1",
    "eslint-config-next": "16.1.6",
    "next": "16.1.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@github/prettier-config": "^0.0.6",
    "@primer/react": "38.3.0",
    "@primer/react-brand": "0.60.1",
    "@types/node": "20.19.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  }
}
